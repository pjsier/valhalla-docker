    "auto": {
      "turn_penalty_factor": 200,
      "search_radius": 50
    },
    "pedestrian": {
      "turn_penalty_factor": 100,
      "search_radius": 25
    },
    "bicycle": {
      "turn_penalty_factor": 140
    },
    "multimodal": {
      "turn_penalty_factor": 70
    },
    "logging": {
      "type": "std_out",
      "color": true
    },
    "service": {
      "proxy": "ipc:///tmp/meili"
    },
    "grid": {
      "size": 500,
      "cache_size": 64
    }
  },
  "tyr": {
    "logging": {
      "type": "std_out",
      "color": true,
      "long_request": 100.0
